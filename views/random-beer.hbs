<ul>
{{#each randomBeers}}
<li>
    <div>
       <h1>{{this.name}} </h1> 

       <img src="{{this.image_url}}" alt="beer photo">
    <p>{{this.description}}</p>
    
    <p>{{this.tagline}}</p>

    <ul>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>

    <p>{{this.brewer_tips}}</p>

    </div>

</li>
{{/each}}
</ul>